<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="de.weingardt.gitlab"
         name="%repository.name"
         point="org.eclipse.mylyn.tasks.ui.repositories">
      <connectorCore
            class="de.weingardt.gitlab.core.GitlabConnector"
            id="de.weingardt.gitlab.core"
            name="Gitlab Connector"/>
      <connectorUi
      		brandingIcon="icons/gitlab-icon.gif"
            class="de.weingardt.gitlab.ui.GitlabConnectorUI"
            id="de.weingardt.gitlab.ui"
            name="Gitlab Connector Ui"
            overlayIcon="icons/gitlab-overlay.gif"/>
   </extension>
   <extension point="org.eclipse.mylyn.tasks.ui.editors">
         <pageFactory
            class="de.weingardt.gitlab.ui.GitlabEditorPageFactory"
            id="de.weingardt.mtrack.ui.editor.pageFactory">
      </pageFactory>
   </extension>
</plugin>
